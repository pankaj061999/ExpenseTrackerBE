{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "jsx": "react",  
      "outDir": "./dist",
      "rootDir": "./src",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true
    },
    "include": ["src"]
  }
  